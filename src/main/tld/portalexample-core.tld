<?xml version="1.0" encoding="UTF-8" ?>
<taglib xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.0" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-jsptaglibrary_2_0.xsd">
  <description><![CDATA["portalexample core tag library"]]></description>
  <display-name>"portalexample core tag library"</display-name>
  <tlib-version>2.2</tlib-version>
  <short-name>vmr</short-name>
  <uri>/portalexample-core</uri>
	
	<tag>
		<description>
			Loads and returns the list of the Cards available in the system.
		</description>
		<name>cards</name>
		<tag-class>org.entando.entando.portalexample.aps.tags.CardListTag</tag-class>
		<body-content>empty</body-content>
		<attribute>
			<description>
				The name of the variable setted in the pagecontext.
			</description>
			<name>listName</name>
			<required>true</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
	</tag>
	
</taglib>